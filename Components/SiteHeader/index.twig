<section class="asm-pre-head">
    <div class="asm-pre-head-row">
        <div class="asm-pre-head-phone">
            <a href="tel:8328340661" aria-label="Main Office (832)834-0661"><span>{% include 'Assets/phone.svg' %}</span> Main Office (832)834-0661</a>
            <a href="tel:7137375529" aria-label="Sales (713)737-5529"><span>{% include 'Assets/phone.svg' %}</span> Sales (713)737-5529</a>
        </div>
        <div class="asm-pre-head-right">
            <nav class="asm-pre-head-nav">
                <ul>
                    {% for item in pre_header_menu.items %}
                        <li class="{{ item.classes | join(' ') }}">
                            <a href="{{ item.link }}">{{ item.title }}</a>
                        </li>
                    {% endfor %}
                </ul>
            </nav>
            <div class="asm-pre-head-social">
                <ul>
                    <li><a href="{{ facebook_url }}" class="asm-pre-head-social-facebook" aria-label="Facebook">{% include 'Assets/facebook.svg' %}</a></li>
                    <li><a href="{{ pinterest_url }}" class="asm-pre-head-social-pinterest" aria-label="Pinterest">{% include 'Assets/pinterest.svg' %}</a></li>
                    <li><a href="{{ instagram_url }}" class="asm-pre-head-social-instagram" aria-label="Instagram">{% include 'Assets/instagram.svg' %}</a></li>
                    <li><a href="{{ youtube_url }}" class="asm-pre-head-social-youtube" aria-label="YouTube">{% include 'Assets/youtube.svg' %}</a></li>
                </ul>
            </div>
        </div>
    </div>
</section>
<header class="asm-head">
    <div class="asm-container">
        <nav class="asm-head-nav">
            <ul>
                {% for item in menu.items %}
                    <li class="{{ item.classes | join(' ') }}">
                        <a href="{{ item.link }}">{{ item.title }}</a>
                        {% if item.children %}
                            <div class="asm-head-nav-sub">
                                <ul>
                                    {% for subitem in item.children %}
                                        <li class="{{ subitem.classes | join(' ') }}">
                                            <a href="{{ subitem.link }}">{{ subitem.title }}</a>
                                                {% if subitem.children %}
                                                    <ul>
                                                        {% for subsubitem in subitem.children %}
                                                            <li class="{{ subsubitem.classes | join(' ') }}">
                                                                <a href="{{ child.link }}">{{ child.title }}</a>
                                                            </li>
                                                        {% endfor %}
                                                    </ul>
                                                {% endif %}
                                            </a>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </div>
                        {% endif %}
                    </li>
                {% endfor %}
            </ul>
        </nav>
    </div>
</header>
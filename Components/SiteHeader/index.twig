<div class="header-wrapper">
  <section class="asm-pre-head">
    <div class="asm-pre-head-row">
      <div class="asm-pre-head-phone">
        <a href="tel:8328340661" aria-label="Main Office (832)834-0661"><img src="{{ phone.src }}" fetchpriority="high" alt="{{ phone.alt }}" width="11" height="11"/> Main Office (832)834-0661</a>
        <a href="tel:7137375529" aria-label="Sales (713)737-5529"><img src="{{ phone.src }}" fetchpriority="high" alt="{{ phone.alt }}" width="11" height="11"/> Sales (713)737-5529</a>
      </div>
      <div class="asm-pre-head-right">
        <nav class="asm-pre-head-nav">
          <ul>
            {% for item in pre_header_menu.items %}
              <li class="{{ item.classes | join(' ') }}">
                <a href="{{ item.link }}">{{ item.title }}</a>
              </li>
            {% endfor %}
          </ul>
        </nav>
        <div class="asm-pre-head-social">
          <ul>
            <li><a href="{{ facebook_url }}" class="asm-pre-head-social-facebook" aria-label="Facebook"><img src="{{ facebook.src }}" fetchpriority="high" alt="{{ facebook.alt }}" /></a></li>
            <li><a href="{{ pinterest_url }}" class="asm-pre-head-social-pinterest" aria-label="Pinterest"><img src="{{ pinterest.src }}" fetchpriority="high" alt="{{ pinterest.alt }}" /></a></li>
            <li><a href="{{ instagram_url }}" class="asm-pre-head-social-instagram" aria-label="Instagram"><img src="{{ instagram.src }}" fetchpriority="high" alt="{{ instagram.alt }}" /></a></li>
            <li><a href="{{ youtube_url }}" class="asm-pre-head-social-youtube" aria-label="YouTube"><img src="{{ youtube.src }}" fetchpriority="high" alt="{{ youtube.alt }}" /></a></li>
          </ul>
        </div>
      </div>
    </div>
  </section>
  <header class="asm-head">
    <div class="asm-head-logo">
      <a href="{{ site.link|e('esc_url') }}">
        <img width="96" height="49" src="{{ logo.src }}" fetchpriority="high" alt="{{ logo.alt }}" />
      </a>
    </div>
    <div class="primary-nav-container">
      <nav class="asm-head-nav">
        <ul>
          {% for item in menu.items %}
            <li class="{{ item.classes | join(' ') }}">
              <a href="{{ item.link }}">
                {{ item.title }}
                {% if item.children %}
                  <div class="asm-head-nav-down-arrow">
                    <img src="{{ down_arrow.src }}" fetchpriority="high" alt="{{ down_arrow.alt }}" />
                  </div>
                {% endif %}
              </a>
              {% if item.children %}
                <div class="asm-head-nav-sub">
                  <ul>
                    {% for subitem in item.children %}
                      <li class="{{ subitem.classes | join(' ') }}">
                        <a href="{{ subitem.link }}">
                          {{ subitem.title }}
                          {% if subitem.children %}
                            <div class="asm-head-nav-right-arrow">
                              <img src="{{ right_arrow.src }}" fetchpriority="high" alt="{{ right_arrow.alt }}" />
                            </div>
                          {% endif %}
                        </a>
                        {% if subitem.children %}
                          <div class="asm-head-nav-sub-sub">
                            <ul>
                              {% for subsubitem in subitem.children %}
                                <li class="{{ subsubitem.classes | join(' ') }}">
                                  <a href="{{ subsubitem.link }}">{{ subsubitem.title }}</a>
                                </li>
                              {% endfor %}
                            </ul>
                          </div>
                        {% endif %}
                      </li>
                    {% endfor %}
                  </ul>
                </div>
              {% endif %}
            </li>
          {% endfor %}
        </ul>
      </nav>
    </div>
    <div class="asm-head-bbb">
      <a target="_blank" href="https://www.bbb.org/us/tx/houston/profile/internet-marketing-services/actual-seo-media-inc-0915-90045053" rel="nofollow">
        <img width="90" height="32" src="{{ bbb.src }}" fetchpriority="high" alt="{{ bbb.alt }}" />
      </a>
    </div>
    <button class="mobile-menu-toggle">
      <div class="hamburger">
        <span></span>
        <span></span>
        <span></span>
      </div>
    </button>
  </header>
  <div class="mobile-menu">
    <button class="mobile-menu-close" aria-label="Close mobile menu">
      <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <line x1="18" y1="6" x2="6" y2="18"></line>
        <line x1="6" y1="6" x2="18" y2="18"></line>
      </svg>
    </button>
    <div id="mobile-menu-content">
      <!-- Main Menu Items -->
      {% for item in menu.items %}
        <div class="mobile-nav-item">
          {% if item.children %}
            <a href="#" class="mobile-nav-link has-dropdown" data-toggle="dropdown">
              {{ item.title }}
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                <polyline points="9 18 15 12 9 6"></polyline>
              </svg>
            </a>
            <div class="mobile-dropdown-menu">
              {% for subitem in item.children %}
                <a href="{{ subitem.link }}" class="mobile-dropdown-item">{{ subitem.title }}</a>
              {% endfor %}
            </div>
          {% else %}
            <a href="{{ item.link }}" class="mobile-nav-link">{{ item.title }}</a>
          {% endif %}
        </div>
      {% endfor %}

      <!-- Pre-header Menu Items -->
      {% if pre_header_menu.items %}
        <div class="mobile-menu-divider"></div>
        {% for item in pre_header_menu.items %}
          <div class="mobile-nav-item">
            <a href="{{ item.link }}" class="mobile-nav-link">{{ item.title }}</a>
          </div>
        {% endfor %}
      {% endif %}

      <!-- Phone Numbers at Bottom -->
      <div class="mobile-menu-divider"></div>
      <div class="mobile-section-heading">Contact Us</div>
      <div class="mobile-menu-contact">
        {% for phone in phoneNumbers %}
          <a href="{{ phone.url }}" aria-label="{{ phone.label }} {{ phone.number }}">
            <span>
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                <path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path>
              </svg>
            </span>
            {{ phone.label }} {{ phone.number }}
          </a>
        {% endfor %}
      </div>
    </div>
  </div>
  <div class="overlay" aria-hidden="true"></div>
</div>